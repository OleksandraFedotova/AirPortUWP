<Page
    x:Class="App1.Pages.AirCraftTypes.CreateAirCraftType"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App1.Pages.AirCraftTypes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <TextBox PlaceholderText="model"  x:Name="Model"/>
            <TextBox PlaceholderText="places"  x:Name="Places"/>
            <TextBox PlaceholderText="loadCapacity"  x:Name="LoadCapacity" />
            <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom">
                <Button Content="Back to AirCraftTypes Page"  Height="80" Margin="10,10,0,0" VerticalAlignment="Bottom" Width="500" Click="BackToList"/>
                <Button Content="Create"  Height="80"  VerticalAlignment="Bottom" Width="500" Click="CreateAirCraftTypeItem"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
